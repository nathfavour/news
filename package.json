{
  "name": "net.ts",
  "version": "1.1.5",
  "type": "module",
  "description": "Node Express Template written in TypeScript NET.ts based on nodebestpractices",
  "main": "src/server.ts",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "server:prod": "node dist/server.js",
    "server:dev": "ts-node-dev --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "test": "jest -c jest.config.js --env=node",
    "test:cov": "jest -c jest.config.js --coverage --env=node",
    "test:watch": "jest -c jest.config.js -i --watchAll",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "prettier": "prettier --check \"{*.js,!(node*)**/*.ts}\"",
    "prettier:fix": "prettier --write \"{*.js,!(node*)**/*.ts}\"",
    "loglevel:change": "runtime-node-refresh"
  },
  "keywords": [
    "express template",
    "node template",
    "node express template",
    "node express template ts",
    "typescript template",
    "express",
    "boilerplate",
    "best practices node",
    "boilerplate node",
    "node ts template"
  ],
  "author": "nowicki.przemek@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@admin-bro/express": "^3.1.0",
    "@admin-bro/mongoose": "^1.1.0",
    "@adminjs/express": "^6.1.0",
    "@adminjs/mongoose": "^4.1.0",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "admin-bro": "^4.0.1",
    "adminjs": "^7.8.7",
    "apicache": "^1.6.3",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "dotenv-cli": "^7.4.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "express": "^4.19.2",
    "express-formidable": "^1.2.0",
    "express-http-context": "^1.2.4",
    "express-mongo-sanitize": "^2.2.0",
    "express-session": "^1.18.0",
    "fs": "^0.0.1-security",
    "http-status": "^1.5.0",
    "joi": "^17.5.0",
    "lusca": "^1.7.0",
    "md5": "^2.3.0",
    "mongoose": "^8.5.1",
    "morgan": "^1.10.0",
    "node-mocks-http": "^1.11.0",
    "normalize-package-data": "^6.0.0",
    "path": "^0.12.7",
    "runtime-node-refresh": "^1.0.6",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.1",
    "tslib": "^2.6.3",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^16.18.101",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "^2.8.8",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0"
  }
}
